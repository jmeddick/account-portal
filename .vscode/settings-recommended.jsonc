  "files.trimFinalNewlines": false,
  // NPM prettier has prettier-plugin-php installed
  "prettier.prettierPath": "./node_modules/prettier",
  "[php]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
  },
}
